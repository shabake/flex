/* 水平间距 */
page {
  height: 100%;
  width: 100%;
  background-color: #FFFFFF;
}
.test-slider {
  width: 100%;
  margin-bottom: 20rpx;
}
.test-text {
  color: #333333;
  font-size: 24rpx;
  margin-bottom: 20rpx;
}
.test-show {
  background-color: #FFFFFF;
  display: flex;
  margin-bottom: 20rpx;
  height: 240rpx;
}
.test-tag {
  display: flex;
  flex-wrap: wrap;
  margin-top: 10rpx;
}
.test-tag .tag {
  height: 50rpx;
  border-radius: 15rpx;
  margin-right: 20rpx;
  margin-bottom: 20rpx;
  background-color: #FFFFFF;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
  border: 1px solid #d5d5d5;
  padding-left: 10rpx;
  padding-right: 10rpx;
}
.test-tag .tag.active {
  background-color: #007AFF;
}
.test-tag .tag.active .text {
  color: #FFFFFF;
}
.test-tag .tag .text {
  line-height: 50rpx;
  color: #666666;
  font-size: 24rpx;
}
.test {
  border-radius: 20rpx;
  background-color: #F7F8FA;
  display: flex;
  flex-direction: column;
  margin: 20rpx;
  padding: 20rpx;
}
.test .test-title {
  font-size: 28rpx;
  color: #333333;
  font-weight: 600;
  margin-bottom: 10rpx;
}
.test .test-view {
  height: 50rpx;
  width: 50rpx;
  font-size: 34rpx;
  font-weight: 600;
  line-height: 50rpx;
  text-align: center;
  color: #FFFFFF;
  margin-right: 20rpx;
  margin-bottom: 20rpx;
}

